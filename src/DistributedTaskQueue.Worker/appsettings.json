{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "StackExchange.Redis": "Warning",
      "DistributedTaskQueue": "Information",
      "DistributedTaskQueue.Worker.Services.WorkerService": "Information",
      "DistributedTaskQueue.Worker.Services.VisibilityTimeoutMonitor": "Information",
      "DistributedTaskQueue.Worker.Services.RetryProcessor": "Information"
    }
  },

  "Worker": {
    "MaxDegreeOfParallelism": 4,
    "VisibilityTimeoutSeconds": 30
  },

  "Retry": {
    "BaseDelaySeconds": 2,
    "MaxDelaySeconds": 120
  },

  "CircuitBreaker": {
    "FailureThreshold": 5,
    "OpenDurationSeconds": 30
  }
}
